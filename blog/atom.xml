---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>MLog</title>
	<link href="http://msfchen.github.io/mlog/atom.xml" rel="self"/>
	<link href="http://msfchen.github.io/mlog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://msfchen.github.io/mlog</id>
	<author>
		<name>M</name>
		<email>sfchen@comcast.net</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://msfchen.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://msfchen.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
